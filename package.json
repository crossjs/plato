{
  "name": "PLATO",
  "version": "3.0.0-beta.10",
  "description": "a Boilerplate for [mobile] SPAs use vue, vuex, vue-router",
  "license": "MIT",
  "main": "src/index.js",
  "scripts": {
    "gfw": "npm i cross-env && cross-env PHANTOMJS_CDNURL=http://npm.taobao.org/mirrors/phantomjs npm install -r https://registry.npm.taobao.org",
    "start": "npm run build && serve ./dist",
    "mock": "cross-env NODE_ENV=development DEBUG=PLATO:* json-server --watch --port 3001 ./mock/db.json",
    "dev": "cross-env NODE_ENV=development DEBUG=PLATO:* webpack-dev-server --progress",
    "e2e": "cross-env NODE_ENV=test DEBUG=PLATO:* node ./test/e2e/runner.js",
    "e2e:dev": "cross-env NODE_ENV=test DEBUG=PLATO:* nodemon ./test/e2e/runner.js",
    "unit": "cross-env NODE_ENV=test DEBUG=PLATO:* karma start ./test/unit/runner.js",
    "unit:dev": "npm run unit -- --watch",
    "lint": "eslint --max-warnings 10 .",
    "lint:fix": "npm run lint -- --fix",
    "lint:css": "stylelint src/**/*.css",
    "test": "npm run lint && npm run lint:css && npm run unit && npm run e2e",
    "clean": "node ./cli/clean",
    "compile": "cross-env NODE_ENV=production DEBUG=PLATO:* node ./cli/compile",
    "build": "npm run test && npm run clean && npm run compile",
    "docs": "bash ./cli/docs.sh",
    "push": "bash ./cli/push.sh"
  },
  "dependencies": {
    "core-js": "^2.4.1",
    "nuo": "^1.0.0",
    "platojs": "*",
    "string-template": "^1.0.0",
    "vuex-actions": "^1.1.1",
    "vuex-localstorage": "^1.0.0",
    "whatwg-fetch": "^2.0.2"
  },
  "devDependencies": {
    "babel-loader": "^6.2.10",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-dynamic-import-webpack": "^1.0.1",
    "babel-plugin-istanbul": "^4.0.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.23.0",
    "bundle-loader": "^0.5.4",
    "chai": "^3.5.0",
    "chromedriver": "^2.27.2",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^3.1.4",
    "cross-spawn": "^5.0.1",
    "css-loader": "^0.26.1",
    "eslint": "^3.15.0",
    "eslint-config-plato": "^0.0.3",
    "eslint-friendly-formatter": "^2.0.7",
    "eslint-loader": "^1.6.1",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "favicons-webpack-plugin": "^0.0.7",
    "file-loader": "^0.10.0",
    "html-webpack-plugin": "^2.28.0",
    "json-server": "^0.9.5",
    "karma": "^1.4.1",
    "karma-coverage": "^1.1.1",
    "karma-es6-shim": "^1.0.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.2",
    "mocha": "^3.2.0",
    "nightwatch": "^0.9.12",
    "nodemon": "^1.11.0",
    "phantomjs-polyfill": "^0.0.2",
    "phantomjs-prebuilt": "^2.1.14",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-cssnext": "^2.9.0",
    "postcss-flexible": "^0.3.0",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^1.3.0",
    "postcss-reporter": "^3.0.0",
    "postcss-rtl": "^0.5.0",
    "postcss-url": "^5.1.2",
    "rimraf": "^2.5.4",
    "selenium-server": "^2.53.1",
    "serve": "^3.3.0",
    "sinon": "^1.17.7",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.1",
    "stylelint": "^7.8.0",
    "stylelint-config-standard": "^16.0.0",
    "url-loader": "^0.5.7",
    "vue-hot-reload-api": "^2.0.8",
    "vue-html-loader": "^1.2.3",
    "vue-loader": "^11.0.0",
    "vue-style-loader": "^2.0.0",
    "vue-template-compiler": "^2.1.10",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0",
    "yargs": "^6.6.0"
  }
}
